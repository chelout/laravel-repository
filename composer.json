{
    "name": "chelout/laravel-repository",
    "description": "Repository implementation for Laravel",
    "homepage": "https://github.com/chelout/laravel-repository",
    "license": "MIT",
    "keywords": [
        "laravel",
        "repository"
    ],
    "authors": [
        {
            "name": "chelout",
            "email": "chelout@gmail.com"
        }
    ],
    "require": {
